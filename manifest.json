{
    "manifest_version": 3,
    "name": "Video Speed and Navigation Controller",
    "version": "1.0",
    "description": "Control video playback speed and navigation on web pages.",
    "icons": {
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}
